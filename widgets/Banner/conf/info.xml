<?xml version="1.0" encoding="UTF-8"?>
<widget version="0.2">
	<title xml:lang="ko">Banner</title>
	<title xml:lang="en">Banner</title>
	<title xml:lang="zh-CN">Banner</title>
	<description xml:lang="ko">3개의 배너 이미지를 롤링하는 위젯입니다</description>
	<description xml:lang="en">widget for images slider</description>
	<description xml:lang="zh-CN">图片滚动小部件</description>
	<version>0.1</version>
	<date>2009-06-10</date>

	<author email_address="developers@xpressengine.com" link="http://www.xpressengine.com">
		<name xml:lang="ko">NHN</name>
		<name xml:lang="en">NHN</name>
		<name xml:lang="zh-CN">NHN</name>
	</author>

	<extra_vars>
        <var id="banner_width">
            <type >text</type>
			<name xml:lang="ko">image width</name>
			<name xml:lang="en">image width</name>
			<name xml:lang="zh-CN">image width</name>
            <description xml:lang="ko">image width(default 1041px) </description>
            <description xml:lang="en">image width(default 1041px) </description>
            <description xml:lang="zh-CN">image width(default 1041px) </description>
        </var>
		<var id="banner_height">
            <type >text</type>
			<name xml:lang="ko">image height</name>
			<name xml:lang="en">image height</name>
			<name xml:lang="zh-CN">image height</name>
            <description xml:lang="ko">image height(default: 960px) </description>
            <description xml:lang="en">image height(default: 960px) </description>
            <description xml:lang="zh-CN">image height(default: 960px) </description>
        </var>
        <var id="banner_1">
            <type filter="jpg,jpeg,gif,png">filebox</type>
			<name xml:lang="ko">한국어 배너 이미지1</name>
			<name xml:lang="en">image 1</name>
			<name xml:lang="zh-CN">横幅图片1</name>
            <description xml:lang="ko">한국어 배너 이미지를 등록해주세요. (가로 960px, 세로 1041px) </description>
        </var>
        <var id="banner_title_1">
            <type>text</type>
            <name xml:lang="ko">제목 #1</name>
            <name xml:lang="en">title for image 1</name>
            <name xml:lang="zh-CN">图片1的标题</name>
            <description xml:lang="ko">배너 이미지에 표시될 제목을 입력해주세요.</description>
        </var>
        <var id="banner_description_1">
            <type>text</type>
            <name xml:lang="ko">설명 #1</name>
            <name xml:lang="en">description for image #1</name>
            <name xml:lang="zh-CN">图片#1的描述</name>
            <description xml:lang="ko">배너 이미지에 표시될 설명을 입력해주세요.</description>
        </var>
        <var id="banner_url_1">
            <type>text</type>
            <name xml:lang="ko">URL #1</name>
            <name xml:lang="en">URL #1</name>
            <name xml:lang="zh-CN">URL #1</name>
            <description xml:lang="ko">배너 이미지에 연결될 URL 1번을 입력해주세요.</description>
