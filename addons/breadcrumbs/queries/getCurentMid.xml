<query id="getCurentMid" action="select">
	<tables>
		<table name="menu_item" />
	</tables>
	<columns>
		<column name="parent_srl" />
		<column name="name" alias="name" />
		<column name="url" alias="module_url" />
	</columns>
	<conditions>
		<condition operation="equal" column="menu_srl" var="menu_srl" />
		<condition operation="equal" column="menu_item_srl" var="menu_item_srl" pipe="and" />
	</conditions>
</query>
